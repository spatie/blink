{
    "name": "spatie/blink",
    "description": "Cache that expires in the blink of an eye",
    "keywords": [
        "spatie",
        "blink",
        "cache",
        "caching"
    ],
    "homepage": "https://github.com/spatie/blink",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-mbstring": "*",
        "psr/simple-cache": "^1.0"
    },
    "provide": {
        "psr/simple-cache-implementation": "1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Blink\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Blink\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
